<property name="clj_address" column="clj_address" not-null="true"></property>
<property name="placement_date" column="placement_date" not-null="true"></property>
<property name="placement_time" column="placement_time" not-null="true"></property>
<property name="contact_tpo" column="contact_tpo" not-null="true"></property>
<property name="interview_department" column="interview_department" not-null="true"></property>
<property name="interviewer_name" column="interviewer_name" not-null="true"></property>



</class>  
  
  
  
  
  
