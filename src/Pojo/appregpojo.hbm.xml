<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
  <class name="Pojo.AppRegPojo" table="applicantregistration">
  
  <id name="Appno" column="usreid">
  <generator class="native"></generator>
  </id>
  
  <property name="username"
  				  column="username"
				  not-null="false"/>
  <property name="password"
  				  column="password"
				  not-null="false"/>
  <property name="name"
  				  column="name"
				  not-null="false"/>
  <property name="gender"
  				  column="gender"
				  not-null="false"/>
  <property name="dob"
  				  column="dob"
				  not-null="false"/>
  <property name="age"
  				  column="age"
				  not-null="false"/>
  <property name="address"
  				  column="address"
				  not-null="false"/>			 
  <property name="city"
  				  column="city"
				  not-null="false"/>				   
<property name="pincode"
  				  column="pincode"
				  not-null="false"/>
<property name="state"
  				  column="state"
				  not-null="false"/>
 <property name="country"
  				  column="country"
				  not-null="false"/>
  <property name="branch"
  				  column="branch"
				  not-null="false"/>
  <property name="college"
  				  column="college"
				  not-null="false"/>
  <property name="mark"
  				  column="mark"
				  not-null="false"/>
  <property name="degree"
  				  column="degree"
				  not-null="false"/>
  <property name="email"
  				  column="email"
				  not-null="false"/>
  <property name="contact"
  				  column="contact"
				  not-null="false"/>
				
  </class>
  </hibernate-mapping>
